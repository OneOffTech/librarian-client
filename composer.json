{
    "name": "oneofftech/librarian-client",
    "description": "OneOffTech Librarian AI service client.",
    "keywords": [
        "oneofftech",
        "librarian-client"
    ],
    "homepage": "https://github.com/oneofftech/librarian-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessio Vertemati",
            "email": "alessio@oneofftech.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "cuyz/valinor": "^2.0",
        "respect/validation": "^2.3",
        "saloonphp/saloon": "^3.6",
        "oneofftech/parse-client": "^0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^11.2.8",
        "spatie/ray": "^1.28",
        "symfony/var-dumper": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "OneOffTech\\LibrarianClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneOffTech\\LibrarianClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --no-coverage",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
